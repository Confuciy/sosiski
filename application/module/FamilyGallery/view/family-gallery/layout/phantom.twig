{% set t_path = "/templates/phantom" %}
{{ docType() }}
<html>
<head>
    <title>{% block title %}Family Gallery{% endblock title %}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    {% block meta %}{% endblock meta %}
    <!--[if lte IE 8]><script src="{{ t_path }}/assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="{{ t_path }}/assets/css/main.css" />
    <!--[if lte IE 9]><link rel="stylesheet" href="{{ t_path }}/assets/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><link rel="stylesheet" href="{{ t_path }}/assets/css/ie8.css" /><![endif]-->
    {% block style %}{% endblock style %}
    {% block script %}{% endblock script %}
</head>
<body>
<!-- Wrapper -->
<div id="wrapper">

    <!-- Header -->
    <header id="header">
        <div class="inner">

            <!-- Logo -->
            {% block logo %}
            <a href="{% block logo_link %}/{% endblock logo_link %}" class="logo">
                {% block logo_img %}<span class="symbol"><img src="{{ t_path }}/images/logo.svg" alt="" /></span>{% endblock logo_img %}{% block logo_title %}<span class="title">Phantom</span>{% endblock logo_title %}
            </a>
            {% endblock logo %}

            <!-- Nav -->
            {% block nav %}
            <nav>
                <ul>
                    <li><a href="#menu">{% block nav_title %}Menu{% endblock nav_title %}</a></li>
                </ul>
            </nav>
            {% endblock nav %}

        </div>
    </header>

    <!-- Menu -->
    {% block menu %}
    <nav id="menu">
        <h2>{% block menu_title %}Menu{% endblock menu_title %}</h2>
        {% block menu_ul %}
        <ul>
            <li><a href="index.html">Home</a></li>
            <li><a href="generic.html">Ipsum veroeros</a></li>
            <li><a href="generic.html">Tempus etiam</a></li>
            <li><a href="generic.html">Consequat dolor</a></li>
            <li><a href="elements.html">Elements</a></li>
        </ul>
        {% endblock menu_ul %}
    </nav>
    {% endblock menu %}

    <!-- Main -->
    {% block main %}
    <div id="main">
        <div class="inner">
            {% block main_header %}
            <header>
                {% block main_header_text %}
                <h1>This is Phantom, a free, fully responsive site<br />
                    template designed by <a href="http://html5up.net">HTML5 UP</a>.</h1>
                {% endblock main_header_text %}
                {% block main_header_subtext %}
                <p>Etiam quis viverra lorem, in semper lorem. Sed nisl arcu euismod sit amet nisi euismod sed cursus arcu elementum ipsum arcu vivamus quis venenatis orci lorem ipsum et magna feugiat veroeros aliquam. Lorem ipsum dolor sit amet nullam dolore.</p>
                {% endblock main_header_subtext %}
            </header>
            {% endblock main_header %}
            {% block tiles %}
            <section class="tiles">{% block tiles_section %}{% endblock tiles_section %}</section>
            {% endblock tiles %}
        </div>
    </div>
    {% endblock main %}

    <!-- Footer -->
    {% block footer %}
    <footer id="footer">
        <div class="inner">
            <section>
                <h2>Get in touch</h2>
                <form method="post" action="#">
                    <div class="field half first">
                        <input type="text" name="name" id="name" placeholder="Name" />
                    </div>
                    <div class="field half">
                        <input type="email" name="email" id="email" placeholder="Email" />
                    </div>
                    <div class="field">
                        <textarea name="message" id="message" placeholder="Message"></textarea>
                    </div>
                    <ul class="actions">
                        <li><input type="submit" value="Send" class="special" /></li>
                    </ul>
                </form>
            </section>
            <section>
                <h2>Follow</h2>
                <ul class="icons">
                    <li><a href="#" class="icon style2 fa-twitter"><span class="label">Twitter</span></a></li>
                    <li><a href="#" class="icon style2 fa-facebook"><span class="label">Facebook</span></a></li>
                    <li><a href="#" class="icon style2 fa-instagram"><span class="label">Instagram</span></a></li>
                    <li><a href="#" class="icon style2 fa-dribbble"><span class="label">Dribbble</span></a></li>
                    <li><a href="#" class="icon style2 fa-github"><span class="label">GitHub</span></a></li>
                    <li><a href="#" class="icon style2 fa-500px"><span class="label">500px</span></a></li>
                    <li><a href="#" class="icon style2 fa-phone"><span class="label">Phone</span></a></li>
                    <li><a href="#" class="icon style2 fa-envelope-o"><span class="label">Email</span></a></li>
                </ul>
            </section>
            <ul class="copyright">
                <li>&copy; Untitled. All rights reserved</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
            </ul>
        </div>
    </footer>
    {% endblock footer %}

</div>

<!-- Scripts -->
{% block footer_js %}
<script src="{{ t_path }}/assets/js/jquery.min.js"></script>
<script src="{{ t_path }}/assets/js/skel.min.js"></script>
<script src="{{ t_path }}/assets/js/util.js"></script>
<!--[if lte IE 8]><script src="{{ t_path }}/assets/js/ie/respond.min.js"></script><![endif]-->
<script src="{{ t_path }}/assets/js/main.js"></script>
{% endblock footer_js %}

</body>
</html>