{% set t_path = "/templates/future-imperfect" %}
{{ docType() }}
<html>
<head>
    <title>{% block title %}Title{% endblock title %}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    {% block meta %}{% endblock meta %}
    <!--[if lte IE 8]><script src="{{ t_path }}/assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="{{ t_path }}/assets/css/main.css" />
    <!--[if lte IE 9]><link rel="stylesheet" href="{{ t_path }}/assets/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><link rel="stylesheet" href="{{ t_path }}/assets/css/ie8.css" /><![endif]-->
    {% block style %}{% endblock style %}
    {% block script %}{% endblock script %}
</head>
<body>

<!-- Wrapper -->
{% block wrapper %}
<div id="wrapper">

    <!-- Header -->
    {% block header %}
    <header id="header">
        <h1><a href="#">Future Imperfect</a></h1>
        <nav class="links">
            <ul>
                <li><a href="#">Lorem</a></li>
                <li><a href="#">Ipsum</a></li>
                <li><a href="#">Feugiat</a></li>
                <li><a href="#">Tempus</a></li>
                <li><a href="#">Adipiscing</a></li>
            </ul>
        </nav>
        <nav class="main">
            <ul>
                <li class="search">
                    <a class="fa-search" href="#search">Search</a>
                    <form id="search" method="get" action="#">
                        <input type="text" name="query" placeholder="Search" />
                    </form>
                </li>
                <li class="menu">
                    <a class="fa-bars" href="#menu">Menu</a>
                </li>
            </ul>
        </nav>
    </header>
    {% endblock header %}

    <!-- Menu -->
    {% block menu %}
    <section id="menu">

        <!-- Search -->
        {% block search %}
        <section>
            <form class="search" method="get" action="#">
                <input type="text" name="query" placeholder="Search" />
            </form>
        </section>
        {% endblock search %}

        <!-- Links -->
        {% block links %}
        <section>
            <ul class="links">
                <li>
                    <a href="#">
                        <h3>Lorem ipsum</h3>
                        <p>Feugiat tempus veroeros dolor</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <h3>Dolor sit amet</h3>
                        <p>Sed vitae justo condimentum</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <h3>Feugiat veroeros</h3>
                        <p>Phasellus sed ultricies mi congue</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <h3>Etiam sed consequat</h3>
                        <p>Porta lectus amet ultricies</p>
                    </a>
                </li>
            </ul>
        </section>
        {% endblock links %}

        <!-- Actions -->
        {% block actions %}
        <section>
            <ul class="actions vertical">
                <li><a href="#" class="button big fit">Log In</a></li>
            </ul>
        </section>
        {% endblock actions %}

    </section>
    {% endblock menu %}

    <!-- Main -->
    <div id="main">

        {% block content %}{{ content|raw }}{% endblock content %}

        <!-- Pagination -->
        {% block pagination %}
        <ul class="actions pagination">
            <li><a href="" class="disabled button big previous">Previous Page</a></li>
            <li><a href="#" class="button big next">Next Page</a></li>
        </ul>
        {% endblock pagination %}

    </div>

    <!-- Sidebar -->
    {% block sidebar %}
    <section id="sidebar">

        <!-- Intro -->
        {% block intro %}
        <section id="intro">
            {% block intro_logo %}<a href="#" class="logo"><img src="{{ t_path }}/images/logo.jpg" alt="" /></a>{% endblock intro_logo %}
            {% block intro_header_block %}
            <header>
                {% block intro_header %}<h2>Family Gallery</h2>{% endblock intro_header %}
                {% block intro_text %}<p>Another fine responsive site template by <a href="http://html5up.net">HTML5 UP</a></p>{% endblock intro_text %}
                {% block intro_icons %}
                <ul class="icons">
                    <li><a href="#" class="fa-twitter"><span class="label">Twitter</span></a></li>
                    <li><a href="#" class="fa-facebook"><span class="label">Facebook</span></a></li>
                    <li><a href="#" class="fa-instagram"><span class="label">Instagram</span></a></li>
                    <li><a href="#" class="fa-rss"><span class="label">RSS</span></a></li>
                    <li><a href="#" class="fa-envelope"><span class="label">Email</span></a></li>
                </ul>
                {% endblock intro_icons %}
            </header>
            {% endblock intro_header_block %}
        </section>
        {% endblock intro %}

        <!-- Mini Posts -->
        {% block mini_posts %}
        <section>
            <div class="mini-posts">

                <!-- Mini Post -->
                <article class="mini-post">
                    <header>
                        <h3><a href="#">Vitae sed condimentum</a></h3>
                        <time class="published" datetime="2015-10-20">October 20, 2015</time>
                        <a href="#" class="author"><img src="{{ t_path }}/images/avatar.jpg" alt="" /></a>
                    </header>
                    <a href="#" class="image"><img src="{{ t_path }}/images/pic04.jpg" alt="" /></a>
                </article>

                <!-- Mini Post -->
                <article class="mini-post">
                    <header>
                        <h3><a href="#">Rutrum neque accumsan</a></h3>
                        <time class="published" datetime="2015-10-19">October 19, 2015</time>
                        <a href="#" class="author"><img src="{{ t_path }}/images/avatar.jpg" alt="" /></a>
                    </header>
                    <a href="#" class="image"><img src="{{ t_path }}/images/pic05.jpg" alt="" /></a>
                </article>

                <!-- Mini Post -->
                <article class="mini-post">
                    <header>
                        <h3><a href="#">Odio congue mattis</a></h3>
                        <time class="published" datetime="2015-10-18">October 18, 2015</time>
                        <a href="#" class="author"><img src="{{ t_path }}/images/avatar.jpg" alt="" /></a>
                    </header>
                    <a href="#" class="image"><img src="{{ t_path }}/images/pic06.jpg" alt="" /></a>
                </article>

                <!-- Mini Post -->
                <article class="mini-post">
                    <header>
                        <h3><a href="#">Enim nisl veroeros</a></h3>
                        <time class="published" datetime="2015-10-17">October 17, 2015</time>
                        <a href="#" class="author"><img src="{{ t_path }}/images/avatar.jpg" alt="" /></a>
                    </header>
                    <a href="#" class="image"><img src="{{ t_path }}/images/pic07.jpg" alt="" /></a>
                </article>

            </div>
        </section>
        {% endblock mini_posts %}

        <!-- Posts List -->
        {% block post_list %}
        <section>
            <ul class="posts">
                <li>
                    <article>
                        <header>
                            <h3><a href="#">Lorem ipsum fermentum ut nisl vitae</a></h3>
                            <time class="published" datetime="2015-10-20">October 20, 2015</time>
                        </header>
                        <a href="#" class="image"><img src="{{ t_path }}/images/pic08.jpg" alt="" /></a>
                    </article>
                </li>
                <li>
                    <article>
                        <header>
                            <h3><a href="#">Convallis maximus nisl mattis nunc id lorem</a></h3>
                            <time class="published" datetime="2015-10-15">October 15, 2015</time>
                        </header>
                        <a href="#" class="image"><img src="{{ t_path }}/images/pic09.jpg" alt="" /></a>
                    </article>
                </li>
                <li>
                    <article>
                        <header>
                            <h3><a href="#">Euismod amet placerat vivamus porttitor</a></h3>
                            <time class="published" datetime="2015-10-10">October 10, 2015</time>
                        </header>
                        <a href="#" class="image"><img src="{{ t_path }}/images/pic10.jpg" alt="" /></a>
                    </article>
                </li>
                <li>
                    <article>
                        <header>
                            <h3><a href="#">Magna enim accumsan tortor cursus ultricies</a></h3>
                            <time class="published" datetime="2015-10-08">October 8, 2015</time>
                        </header>
                        <a href="#" class="image"><img src="{{ t_path }}/images/pic11.jpg" alt="" /></a>
                    </article>
                </li>
                <li>
                    <article>
                        <header>
                            <h3><a href="#">Congue ullam corper lorem ipsum dolor</a></h3>
                            <time class="published" datetime="2015-10-06">October 7, 2015</time>
                        </header>
                        <a href="#" class="image"><img src="{{ t_path }}/images/pic12.jpg" alt="" /></a>
                    </article>
                </li>
            </ul>
        </section>
        {% endblock post_list %}

        <!-- About -->
        {% block about %}
        <section class="blurb">
            <h2>About</h2>
            <p>Mauris neque quam, fermentum ut nisl vitae, convallis maximus nisl. Sed mattis nunc id lorem euismod amet placerat. Vivamus porttitor magna enim, ac accumsan tortor cursus at phasellus sed ultricies.</p>
            <ul class="actions">
                <li><a href="#" class="button">Learn More</a></li>
            </ul>
        </section>
        {% endblock about %}

        <!-- Footer -->
        {#
        {% block footer %}
        <section id="footer">
            <ul class="icons">
                <li><a href="#" class="fa-twitter"><span class="label">Twitter</span></a></li>
                <li><a href="#" class="fa-facebook"><span class="label">Facebook</span></a></li>
                <li><a href="#" class="fa-instagram"><span class="label">Instagram</span></a></li>
                <li><a href="#" class="fa-rss"><span class="label">RSS</span></a></li>
                <li><a href="#" class="fa-envelope"><span class="label">Email</span></a></li>
            </ul>
            {% block copyright %}<p class="copyright">&copy; Untitled. Design: <a href="http://html5up.net">HTML5 UP</a>. Images: <a href="http://unsplash.com">Unsplash</a>.</p>{% endblock copyright %}
        </section>
        {% endblock footer %}
        #}

    </section>
    {% endblock sidebar %}

</div>
{% endblock wrapper %}

<!-- Scripts -->
{% block footer_js %}
<script src="{{ t_path }}/assets/js/jquery.min.js"></script>
<script src="{{ t_path }}/assets/js/skel.min.js"></script>
<script src="{{ t_path }}/assets/js/util.js"></script>
<!--[if lte IE 8]><script src="{{ t_path }}/assets/js/ie/respond.min.js"></script><![endif]-->
<script src="{{ t_path }}/assets/js/main.js"></script>
{% endblock footer_js %}
</body>
</html>